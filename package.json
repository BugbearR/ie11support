{
  "name": "@bugbearr/ie11support",
  "version": "0.0.1",
  "description": "IE11 Support",
  "main": "esm/index.js",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-minify": "^0.5.1",
    "cross-env": "^7.0.2",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "files": [
    "esm",
    "min",
    "umd",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "clean": "run-s clean:min clean:umd",
    "clean:min": "rimraf min",
    "clean:umd": "rimraf umd",
    "build": "run-s clean:umd build:umd clean:min build:min",
    "build:min": "cross-env NODE_ENV=production babel esm --out-dir min",
    "build:umd": "cross-env NODE_ENV=development babel esm --out-dir umd",
    "test": "jest"
  },
  "unpkg": "min/index.min.js"
}
