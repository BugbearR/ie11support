{
  "name": "@bugbearr/ie11support",
  "description": "IE11 Support",
  "version": "0.0.5",
  "browserslist": [
    "ie 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@types/jest": "^26.0.14",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-minify": "^0.5.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "jest": "^26.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.3"
  },
  "files": [
    "src",
    "esm",
    "min",
    "umd",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "license": "CC0-1.0",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:bugbearr/ie11support",
  "scripts": {
    "build": "run-s clean:esm build:esm clean:lib build:lib clean:umd build:umd",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:umd": "cross-env NODE_ENV=production babel esm --out-dir umd",
    "clean": "run-s clean:esm clean:lib clean:umd",
    "clean:esm": "rimraf esm",
    "clean:lib": "rimraf lib",
    "clean:umd": "rimraf umd",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "run-s test build",
    "test": "jest"
  },
  "unpkg": "umd/index.js"
}
